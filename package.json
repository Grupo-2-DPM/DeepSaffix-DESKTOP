{
  "name": "deepsaffix-cliente",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Gerbetwo <lassomoluna9@gmail.com>",
  "main": "dist-electron/main.js",
  "scripts": {
    "compile:electron": "tsc -p tsconfig.electron.json",
    "dev": "concurrently -k \"npm run dev\" \"npm run compile:electron",
    "dev:electron": "electron . --no-sandbox",
    "build:linux": "npm run compile:electron && electron-builder --linux",
    "build:win": "npm run compile:electron && electron-builder --win",
    "build:mac": "npm run compile:electron && electron-builder --mac"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/electron": "^1.4.38",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}